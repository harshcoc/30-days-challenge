find the smallest number in array 

  #include <iostream>
#include <climits>
using namespace std ; 
int main() {
   int num [ ] = {5,15,6,23,17} ;
   int size = 5,  smallest=INT_MAX; ;
   
   
   for (int i =0 ; i< size ;i++)
   {
       if(num [i] <smallest)
       {
           smallest = num[i] ;
       }
   }
   
   cout << "the smallest num is " << smallest << endl ;

    return 0;
}
